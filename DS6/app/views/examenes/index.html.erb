<p id="notice"><%= notice %></p>

<h1>Examenes</h1>

<table id="tabla">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Fecha</th>
      <th>Asignaturas</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @examenes.each do |examene| %>
      <tr>
        <td><%= examene.nombre %></td>
        <td><%= examene.fecha %></td>
        <td><%= examene.asignaturas_id %></td>
        <td><%= link_to 'Show', examene %></td>
        <td><%= link_to 'Edit', edit_examene_path(examene) %></td>
        <td><%= link_to 'Destroy', examene, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Examene', new_examene_path %>
