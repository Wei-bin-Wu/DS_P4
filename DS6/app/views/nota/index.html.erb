<p id="notice"><%= notice %></p>

<h1>Nota</h1>

<table id="tabla">
  <thead>
    <tr>
      <th>Alumnos</th>
      <th>Examenes</th>
      <th>Nota</th>
      <th>Foto</th>
      <th colspan="3">Acciones</th>
    </tr>
  </thead>

  <tbody>
    <% @nota.each do |notum| %>
      <tr>
        <td><%= notum.alumnos_id %></td>
        <td><%= notum.examenes_id %></td>
        <td><%= notum.nota %></td>
        <td><%= link_to 'Descargar Foto', asset_path(notum.foto) %></td>
        <td><%= link_to 'Show', notum %></td>
        <td><%= link_to 'Edit', edit_notum_path(notum) %></td>
        <td><%= link_to 'Destroy', notum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nota', new_notum_path %>
