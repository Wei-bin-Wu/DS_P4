<!DOCTYPE html>
<html>
  <head>
    <title>DS6</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= link_to "Profesores", root_path %>
    <%= link_to "Alumnos", :alumno %>
    <%= link_to "Administrador", :administrador %>
    <%= link_to "Asignaturas", :asignatura %>
    <%= link_to "Matriculas", :matricula %>
    <%= link_to "Examenes", :examene %>
    <%= link_to "Nota", :nota %>
    <%= yield %>
  </body>
</html>
